            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>

    <fragment
        android:id="@+id/fragmentMap"
        android:name="com.myroute.fragments.FragmentMap"
        android:label="fragment_map"
        tools:layout="@layout/fragment_map">
        <action
            android:id="@+id/action_fragmentMap_to_fragmentCamiones"
            app:destination="@id/fragmentCamiones"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_left" />
        <action
            android:id="@+id/action_fragmentMap_to_fragmentTrenes"
            app:destination="@id/fragmentTrenes"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_right"
