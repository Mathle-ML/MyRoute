            app:exitAnim="@anim/slide_out_right"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/fragmentCamiones"
        android:name="com.myroute.fragments.FragmentCamiones"
        android:label="FragmentCamiones">
